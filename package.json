  "module": "es/index.js",
  "files": [
    "es",
    "index.js",
    "umd"
  ],
  "author": "beardedtim <timiroberts@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "lenses"
  ],
  "scripts": {
